<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Members">
    <select id="login" resultType="com.kedu.dto.MembersDTO">
        SELECT
            *
        FROM
            members
        WHERE
            id = #{id}
        AND
            pw = #{pw}
    </select>
</mapper>